{
  "name": "copy-helper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prisma:clean": "rm -rf node_modules/.prisma node_modules/@prisma/client || true",
    "prisma:generate": "prisma generate --no-engine",
    "prisma:check": "node scripts/check-prisma.js",
    "build": "npm run prisma:clean && npm run prisma:generate && npm run prisma:check && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "openai": "^4.47.0",
    "@prisma/client": "^5.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.5.0",
    "prisma": "^5.16.0",
    "tailwindcss": "^3.4.0",
    "postcss": "^8.4.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0"
  }
}
